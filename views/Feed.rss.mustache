<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title>{{_.title}}</title>
		<description>{{_.subtitle}}</description>
		<link>{{_.base_abs}}</link>
		<atom:link href="{{_.base_abs}}{{_.feed.rss}}" rel="self" type="application/rss+xml" />
		<lastBuildDate>{{date.rss}}</lastBuildDate>
 
		{{#item}}
		<item>
			<link>{{_.base_abs}}{{link}}</link>
			<guid isPermaLink="true">{{_.base_abs}}{{link}}</guid>

			<title>{{title}}</title>
			<description>{{#_.description}}{{summary}}{{/_.description}}</description>
		</item>
		{{/item}}
 
	</channel>
</rss>